{
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "2.1",
  "manifest_version": 2,
  "default_locale": "ru",
  "homepage_url": "http://github.com/arteezy/mk-in-order",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "*://moikrug.ru/*",
    "*://freelansim.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://moikrug.ru/*",
        "*://freelansim.ru/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "lib/jquery-observe.min.js",
        "lib/lodash.min.js",
        "src/sortjobs.js"
      ]
    }
  ]
}
