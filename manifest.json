{
  "name": "Мой Круг в порядке",
  "version": "2.0",
  "manifest_version": 2,
  "description": "Сортирует вакансии и заказы на сайтах moikrug.ru и freelansim.ru по дате",
  "homepage_url": "http://github.com/arteezy/mk-in-order",
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://moikrug.ru/*",
    "https://freelansim.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://moikrug.ru/*",
        "https://freelansim.ru/*"
      ],
      "js": [
        "js/jquery.min.js",
        "js/jquery-observe.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
