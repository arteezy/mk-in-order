{
  "name": "Мой Круг в порядке",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Sorts the jobs listings on moikrug.ru, so the newest jobs are shown first",
  "homepage_url": "http://github.com/arteezy/mk-in-order",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://moikrug.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.moikrug.ru/*"
      ],
      "js": [
        "js/jquery/jquery.min.js",
        "js/jquery-observe.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
