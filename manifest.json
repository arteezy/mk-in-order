{
  "name": "moikrug",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Shows newest jobs on moikrug",
  "homepage_url": "http://github.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "https://moikrug.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.moikrug.ru/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
